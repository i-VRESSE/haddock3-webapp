    "isbot": "^3.6.5",
    "jose": "^4.13.1",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@ltd/j-toml": "^1.38.0",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/js-yaml": "^4.0.5",
    "@types/jszip": "^3.4.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@vitest/coverage-c8": "^0.29.1",
