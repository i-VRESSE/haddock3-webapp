{
  "name": "haddock3-webapp",
  "private": true,
  "version": "0.2.0",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "catalogs:easy": "curl -L https://github.com/i-VRESSE/workflow-builder/raw/${WBTAG:=main}/packages/haddock3_catalog/public/catalog/haddock3.easy.yaml | npx js-yaml > app/catalogs/haddock3.easy.json",
    "catalogs:expert": "curl -L https://github.com/i-VRESSE/workflow-builder/raw/${WBTAG:=main}/packages/haddock3_catalog/public/catalog/haddock3.expert.yaml | npx js-yaml > app/catalogs/haddock3.expert.json",
    "catalogs:guru": "curl -L https://github.com/i-VRESSE/workflow-builder/raw/${WBTAG:=main}/packages/haddock3_catalog/public/catalog/haddock3.guru.yaml  | npx js-yaml > app/catalogs/haddock3.guru.json",
    "catalogs": "npm run catalogs:easy && npm run catalogs:expert && npm run catalogs:guru && npx prettier --write app/catalogs/*.json",
    "dev": "remix dev --manual",
    "format": "prettier --write .",
    "generate-client": "node app/bartender-client/generate.mjs && npx prettier --write app/bartender-client/bartenderschema.d.ts",
    "generate-client:restraints": "node app/haddock3-restraints-client/generate.mjs && npx prettier --write app/haddock3-restraints-client/schema.d.ts",
    "lint": "eslint  --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/index.js",
    "test": "vitest",
    "typecheck": "tsc",
    "generate:migration": "drizzle-kit generate:pg",
    "setup": "tsx app/drizzle/migrate.server.ts",
    "docker:dev": "docker compose -f deploy/docker-compose.dev.yml up",
    "docker:devrm": "docker compose -f deploy/docker-compose.dev.yml down -v",
    "psql:dev": "docker compose -f deploy/docker-compose.dev.yml exec userdb psql -U postgres -d postgres"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/.sessions",
    "/coverage"
  ],
  "dependencies": {
    "@i-vresse/haddock3-analysis-components": "^0.4.1",
    "@i-vresse/wb-core": "^2.0.1",
    "@i-vresse/wb-form": "^1.1.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@remix-run/css-bundle": "^2.5.1",
    "@remix-run/node": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "@remix-run/serve": "^2.5.1",
    "@sentry/node": "^7.99.0",
    "@tanstack/react-table": "^8.13.2",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "d3-dsv": "^3.0.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.29.4",
    "fflate": "^0.8.2",
    "isbot": "^4.1.0",
    "jose": "^4.13.1",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.321.0",
    "ngl": "^2.2.2",
    "openapi-fetch": "^0.9.3",
    "plotly.js": "^2.26.1",
    "postgres": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plotly.js": "^2.6.0",
    "remix-auth": "^3.6.0",
    "remix-auth-form": "^1.3.0",
    "remix-auth-oauth2": "^1.8.0",
    "remix-themes": "^1.3.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "tiny-invariant": "^1.3.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.1",
    "valibot": "^0.20.1"
  },
  "devDependencies": {
    "@ltd/j-toml": "^1.38.0",
    "@playwright/test": "^1.43.0",
    "@remix-run/dev": "^2.5.1",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.5.1",
    "@types/d3-dsv": "^3.0.2",
    "@types/eslint": "^8.37.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/react-plotly.js": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@vitest/coverage-v8": "^1.2.0",
    "autoprefixer": "^10.4.17",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^9.20.3",
    "openapi-typescript": "^7.0.0-next.5",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
